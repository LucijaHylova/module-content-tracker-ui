{
  "name": "module-content-tracker-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:developement": "ng serve -c development --proxy-config src/proxy.conf.json",
    "start:test": "ng serve -c test --proxy-config src/proxy.conf.json",
    "start:railway": "ng serve -c railway",
    "start": "ng run module-content-tracker-ui:start:railway",
    "build:dev": "ng build -c development",
    "build:prod": "ng build -c production",
    "build": "ng build --configuration railway",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e": "npx playwright test --ui show-report --config=playwright.config.ts",
    "e2e:test": "npx playwright test show-report --config=playwright.config.ts --ui",
    "run:codepen": "npx playwright codegen http://localhost:4200",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.0",
    "@angular/cdk": "^20.2.3",
    "@angular/common": "^20.3.2",
    "@angular/compiler": "^20.3.2",
    "@angular/core": "^20.3.2",
    "@angular/forms": "^20.3.2",
    "@angular/material": "^20.2.3",
    "@angular/platform-browser": "^20.3.2",
    "@angular/platform-browser-dynamic": "^20.3.0",
    "@angular/router": "^20.3.2",
    "@jsverse/transloco": "^7.6.1",
    "@ngrx/operators": "^20.0.1",
    "@ngrx/signals": "^20.0.1",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "date-fns": "^4.1.0",
    "echarts": "^6.0.0",
    "https-proxy-agent": "^7.0.6",
    "material-symbols": "^0.35.1",
    "ngx-echarts": "^20.0.2",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^2.0.0-alpha.24",
    "@analogjs/vitest-angular": "^2.0.0-alpha.24",
    "@angular/build": "^20.3.3",
    "@angular/cli": "^20.3.3",
    "@angular/compiler-cli": "^20.3.2",
    "@angular/localize": "^20.3.2",
    "@playwright/test": "^1.56.0",
    "@softarc/eslint-plugin-sheriff": "^0.19.0",
    "@softarc/sheriff-core": "^0.19.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.4.0",
    "@vitest/coverage-v8": "^3.2.4",
    "angular-eslint": "^20.3.0",
    "eslint": "^9.35.0",
    "jsdom": "^27.0.0",
    "ng-mocks": "^14.13.5",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vitest": "^3.2.4"
  }
}
